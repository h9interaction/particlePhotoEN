        });

        html += `</tbody></table></div>`;

        if (csvData.length > maxPreviewRows) {
            html += `<p style="font-size: 12px; color: #666; margin-top: 8px;">... ë° ${csvData.length - maxPreviewRows}ê°œ í–‰ ë”</p>`;
        }

        previewContainer.innerHTML = html;
    }

    // ë²Œí¬ ì—…ë¡œë“œ ì‹œì‘
    async startBulkUpload() {
        if (!this.selectedCsvData) {
            this.showMessage('ì—…ë¡œë“œí•  ë°ì´í„°ê°€ ì—†ìŠµë‹ˆë‹¤.', 'error');
            return;
        }

        const conflictMode = document.querySelector('input[name="conflictMode"]:checked').value;
        
        this.showUploadStep(3);
        
        try {
            // CSV ë°ì´í„°ë¥¼ ë‹¤ì‹œ CSV ë¬¸ìì—´ë¡œ ë³€í™˜
            const headers = Object.keys(this.selectedCsvData[0]);
            const csvContent = [
                headers.join(','),
                ...this.selectedCsvData.map(row => 
                    headers.map(header => `"${row[header] || ''}"`).join(',')
                )
            ].join('\n');

            // FormData ìƒì„±
            const formData = new FormData();
            const blob = new Blob(['\uFEFF' + csvContent], { type: 'text/csv;charset=utf-8;' });
            formData.append('csvFile', blob, 'upload.csv');
            formData.append('conflictMode', conflictMode);

            // ì§„í–‰ë¥  í‘œì‹œ
            const progressFill = document.getElementById('progressFill');
            const progressText = document.getElementById('progressText');
            
            progressText.textContent = 'ì—…ë¡œë“œ ì¤‘...';
            progressFill.style.width = '50%';

            // API í˜¸ì¶œ
            const response = await fetch('/api/people/bulk-upload', {
                method: 'POST',
                body: formData
            });

            const result = await response.json();

            if (response.ok) {
                // ì„±ê³µ
                progressFill.style.width = '100%';
                progressText.textContent = 'ì™„ë£Œ';
                
                // ê²°ê³¼ í‘œì‹œ
                this.showUploadResults(result.results);
                
                // ë°ì´í„° ìƒˆë¡œê³ ì¹¨
                await this.loadPeople();
                this.renderItems();
                
                this.showMessage(result.message, 'success');
            } else {
                throw new Error(result.error || 'ì—…ë¡œë“œ ì‹¤íŒ¨');
            }

        } catch (error) {
            console.error('ì—…ë¡œë“œ ì˜¤ë¥˜:', error);
            this.showUploadError(error.message);
        }
    }

    // ì—…ë¡œë“œ ê²°ê³¼ í‘œì‹œ
    showUploadResults(results) {
        const uploadResults = document.getElementById('uploadResults');
        const resultSummary = document.getElementById('resultSummary');
        const uploadCloseBtn = document.getElementById('uploadCloseBtn');

        let html = `
            <div style="background: #f9f9f9; padding: 15px; border-radius: 4px; margin: 15px 0;">
                <p><strong>ì—…ë¡œë“œ ì™„ë£Œ</strong></p>
                <ul style="margin: 10px 0; padding-left: 20px;">
                    <li>ì „ì²´: ${results.total}ê°œ</li>
                    <li style="color: #27ae60;">ì„±ê³µ: ${results.success}ê°œ</li>
                    <li style="color: #f39c12;">ê±´ë„ˆëœ€: ${results.skipped}ê°œ</li>
                    <li style="color: #e74c3c;">ì‹¤íŒ¨: ${results.failed}ê°œ</li>
                </ul>
        `;

        if (results.errors && results.errors.length > 0) {
            html += `
                <details style="margin-top: 10px;">
                    <summary style="cursor: pointer; color: #e74c3c;">ì˜¤ë¥˜ ìƒì„¸ (${results.errors.length}ê°œ)</summary>
                    <ul style="margin: 10px 0; padding-left: 20px; font-size: 12px;">
            `;
            results.errors.forEach(error => {

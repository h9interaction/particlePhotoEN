/**
 * ImageWorkerManager.js
 * ImageProcessorWorkerì™€ ë©”ì¸ ìŠ¤ë ˆë“œ ê°„ì˜ í†µì‹ ì„ ê´€ë¦¬í•˜ëŠ” í´ë˜ìŠ¤
 * Promise ê¸°ë°˜ ë¹„ë™ê¸° ì²˜ë¦¬ ë° ì‘ì—… í ê´€ë¦¬
 */

class ImageWorkerManager {
    constructor() {
        this.worker = null;
        this.pendingTasks = new Map();
        this.taskIdCounter = 0;
        this.isInitialized = false;
        this.retryCount = 3;
        
        this.initWorker();
    }
    
    /**
     * ì›Œì»¤ ì´ˆê¸°í™”
     */
    initWorker() {
        try {
            this.worker = new Worker('./workers/ImageProcessorWorker.js');
            this.worker.onmessage = this.handleWorkerMessage.bind(this);
            this.worker.onerror = this.handleWorkerError.bind(this);
            
            // ì›Œì»¤ ìƒì¡´ í™•ì¸
            this.pingWorker().then(() => {
                this.isInitialized = true;
                console.log('ImageWorkerManager ì´ˆê¸°í™” ì™„ë£Œ');
            }).catch((error) => {
                console.error('ì›Œì»¤ ì´ˆê¸°í™” ì‹¤íŒ¨:', error);
                this.fallbackToMainThread = true;
            });
            
        } catch (error) {
            console.error('ì›¹ ì›Œì»¤ ìƒì„± ì‹¤íŒ¨:', error);
            this.fallbackToMainThread = true;
        }
    }
    
    /**
     * ì›Œì»¤ ìƒì¡´ í™•ì¸
     * @returns {Promise} í•‘ ê²°ê³¼
     */
    pingWorker() {
        return this.sendMessage('PING', {});
    }
    
    /**
     * ì´ë¯¸ì§€ ë°ì´í„°ë¥¼ ë¹„ë™ê¸°ë¡œ ì²˜ë¦¬
     * @param {ImageData} imageData - ì²˜ë¦¬í•  ì´ë¯¸ì§€ ë°ì´í„°
     * @param {number} stepPixel - í”½ì…€ ìŠ¤í…
     * @param {number} canvasWidth - ìº”ë²„ìŠ¤ ë„ˆë¹„
     * @param {number} canvasHeight - ìº”ë²„ìŠ¤ ë†’ì´
     * @param {Object} cropInfo - í¬ë¡­ ì •ë³´
     * @returns {Promise<Array>} ì²˜ë¦¬ëœ í”½ì…€ ë°ì´í„°
     */
    async processImageAsync(imageData, stepPixel, canvasWidth, canvasHeight, cropInfo = {}) {
        // ì›Œì»¤ê°€ ì´ˆê¸°í™”ë˜ì§€ ì•Šì•˜ê±°ë‚˜ í´ë°± ëª¨ë“œì¸ ê²½ìš° ë©”ì¸ ìŠ¤ë ˆë“œì—ì„œ ì²˜ë¦¬
        if (!this.isInitialized || this.fallbackToMainThread) {
            return this.processImageMainThread(imageData, stepPixel, canvasWidth, canvasHeight, cropInfo);
        }
        
        try {
            const result = await this.sendMessage('PROCESS_IMAGE', {
                imageData: {
                    data: imageData.data,
                    width: imageData.width,
                    height: imageData.height
                },
                stepPixel,
                canvasWidth,
                canvasHeight,
                offsetX: cropInfo.offsetX || 0,
                offsetY: cropInfo.offsetY || 0,
                drawWidth: cropInfo.drawWidth || canvasWidth,
                drawHeight: cropInfo.drawHeight || canvasHeight,
                startTime: performance.now()
            });
            
            return result.pixelData;
            
        } catch (error) {
            console.warn('ì›Œì»¤ì—ì„œ ì´ë¯¸ì§€ ì²˜ë¦¬ ì‹¤íŒ¨, ë©”ì¸ ìŠ¤ë ˆë“œë¡œ í´ë°±:', error);
            return this.processImageMainThread(imageData, stepPixel, canvasWidth, canvasHeight, cropInfo);
        }
    }
    
    /**
     * ë©”ì¸ ìŠ¤ë ˆë“œì—ì„œ ì´ë¯¸ì§€ ì²˜ë¦¬ (í´ë°±)
     * @param {ImageData} imageData - ì²˜ë¦¬í•  ì´ë¯¸ì§€ ë°ì´í„°
     * @param {number} stepPixel - í”½ì…€ ìŠ¤í…  
     * @param {number} canvasWidth - ìº”ë²„ìŠ¤ ë„ˆë¹„
     * @param {number} canvasHeight - ìº”ë²„ìŠ¤ ë†’ì´
     * @param {Object} cropInfo - í¬ë¡­ ì •ë³´
     * @returns {Array} ì²˜ë¦¬ëœ í”½ì…€ ë°ì´í„°
     */
    processImageMainThread(imageData, stepPixel, canvasWidth, canvasHeight, cropInfo = {}) {
        const pixelData = [];
        const data = imageData.data;
        const { offsetX = 0, offsetY = 0, drawWidth = canvasWidth, drawHeight = canvasHeight } = cropInfo;
        
        // ê¸°ì¡´ imageLoader.jsì™€ ë™ì¼í•œ ë¡œì§
        for (let y = 0; y < drawHeight; y += stepPixel) {
            for (let x = 0; x < drawWidth; x += stepPixel) {
                const actualX = Math.floor(x + offsetX);
                const actualY = Math.floor(y + offsetY);
                
                if (actualX >= 0 && actualY >= 0 && actualX < canvasWidth && actualY < canvasHeight) {
                    const idx = (actualY * canvasWidth + actualX) * 4;
                    
                    if (idx < data.length - 2) {
                        const r = data[idx];
                        const g = data[idx + 1];
                        const b = data[idx + 2];
                        
                        // ìƒ‰ìƒ ë°˜ì „
                        const invertedR = 255 - r;
                        const invertedG = 255 - g;
                        const invertedB = 255 - b;
                        
                        pixelData.push({
                            x: actualX,
                            y: actualY,
                            color: { r: invertedR, g: invertedG, b: invertedB }
                        });
                    }
                }
            }
        }
        
        return pixelData;
    }
    
    /**
     * ì›Œì»¤ì—ê²Œ ë©”ì‹œì§€ ì „ì†¡
     * @param {string} type - ë©”ì‹œì§€ íƒ€ì…
     * @param {Object} data - ì „ì†¡í•  ë°ì´í„°
     * @returns {Promise} ì‘ë‹µ Promise
     */
    sendMessage(type, data) {
        return new Promise((resolve, reject) => {
            const taskId = this.generateTaskId();
            const timeout = setTimeout(() => {
                this.pendingTasks.delete(taskId);
                reject(new Error(`ì›Œì»¤ ì‘ë‹µ ì‹œê°„ ì´ˆê³¼: ${type}`));
            }, 10000); // 10ì´ˆ íƒ€ì„ì•„ì›ƒ
            
            this.pendingTasks.set(taskId, { resolve, reject, timeout });
            
            this.worker.postMessage({
                type,
                data,
                taskId
            });
        });
    }
    
    /**
     * ì›Œì»¤ ë©”ì‹œì§€ ì²˜ë¦¬
     * @param {MessageEvent} event - ì›Œì»¤ ë©”ì‹œì§€ ì´ë²¤íŠ¸
     */
    handleWorkerMessage(event) {
        const { type, data, taskId } = event.data;
        
        const task = this.pendingTasks.get(taskId);
        if (!task) {
            console.warn('ì•Œ ìˆ˜ ì—†ëŠ” ì‘ì—… ID:', taskId);
            return;
        }
        
        clearTimeout(task.timeout);
        this.pendingTasks.delete(taskId);
        
        if (type === 'ERROR') {
            task.reject(new Error(data.error));
        } else {
            task.resolve(data);
        }
    }
    
    /**
     * ì›Œì»¤ ì—ëŸ¬ ì²˜ë¦¬
     * @param {ErrorEvent} error - ì›Œì»¤ ì—ëŸ¬ ì´ë²¤íŠ¸
     */
    handleWorkerError(error) {
        console.error('ImageProcessorWorker ì—ëŸ¬:', error);
        
        // ëª¨ë“  ëŒ€ê¸° ì¤‘ì¸ ì‘ì—…ì„ ì‹¤íŒ¨ë¡œ ì²˜ë¦¬
        this.pendingTasks.forEach((task) => {
            clearTimeout(task.timeout);
            task.reject(new Error('ì›Œì»¤ ì—ëŸ¬ ë°œìƒ'));
        });
        this.pendingTasks.clear();
        
        // í´ë°± ëª¨ë“œë¡œ ì „í™˜
        this.fallbackToMainThread = true;
    }
    
    /**
     * ê³ ìœ  ì‘ì—… ID ìƒì„±
     * @returns {string} ì‘ì—… ID
     */
    generateTaskId() {
        return `task_${++this.taskIdCounter}_${Date.now()}`;
    }
    
    /**
     * ì›Œì»¤ ì •ë¦¬
     */
    cleanup() {
        if (this.worker) {
            this.worker.terminate();
            this.worker = null;
        }
        
        // ëª¨ë“  ëŒ€ê¸° ì¤‘ì¸ ì‘ì—… ì·¨ì†Œ
        this.pendingTasks.forEach((task) => {
            clearTimeout(task.timeout);
            task.reject(new Error('ì›Œì»¤ê°€ ì¢…ë£Œë¨'));
        });
        this.pendingTasks.clear();
        
        console.log('ImageWorkerManager ì •ë¦¬ ì™„ë£Œ');
    }
    
    /**
     * í˜„ì¬ ìƒíƒœ ì •ë³´ ë°˜í™˜
     * @returns {Object} ìƒíƒœ ì •ë³´
     */
    getStatus() {
        return {
            isInitialized: this.isInitialized,
            fallbackMode: this.fallbackToMainThread,
            pendingTasks: this.pendingTasks.size,
            workerAvailable: !!this.worker
        };
    }
}

// ì‹±ê¸€í†¤ ì¸ìŠ¤í„´ìŠ¤
const imageWorkerManager = new ImageWorkerManager();

export default imageWorkerManager;

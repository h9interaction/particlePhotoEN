const uploadProgress = new Map();

// SSEë¥¼ ìœ„í•œ ì§„í–‰ ìƒí™© ìŠ¤íŠ¸ë¦¼ API
app.get('/api/upload-progress/:sessionId', (req, res) => {
    const sessionId = req.params.sessionId;
    
    res.writeHead(200, {
        'Content-Type': 'text/event-stream',
        'Cache-Control': 'no-cache',
        'Connection': 'keep-alive',
        'Access-Control-Allow-Origin': '*',
        'Access-Control-Allow-Headers': 'Cache-Control'
    });

    // ì¦‰ì‹œ ì—°ê²° í™•ì¸ ë©”ì‹œì§€ ì „ì†¡
    res.write(`data: ${JSON.stringify({ type: 'connected', sessionId })}\n\n`);

    // ì§„í–‰ ìƒí™© ì—…ë°ì´íŠ¸ë¥¼ í™•ì¸í•˜ëŠ” ì¸í„°ë²Œ
    const interval = setInterval(() => {
        const progress = uploadProgress.get(sessionId);
        if (progress) {
            res.write(`data: ${JSON.stringify(progress)}\n\n`);
            
            // ì™„ë£Œë˜ë©´ ì„¸ì…˜ ì •ë¦¬
            if (progress.type === 'completed' || progress.type === 'error') {
                uploadProgress.delete(sessionId);
                clearInterval(interval);
                res.end();
            }
        }
    }, 100);

    // í´ë¼ì´ì–¸íŠ¸ ì—°ê²° ì¢…ë£Œ ì²˜ë¦¬
    req.on('close', () => {
        clearInterval(interval);
        uploadProgress.delete(sessionId);
    });
});


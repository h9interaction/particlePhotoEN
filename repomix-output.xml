/**
 * OptimizedParticlePool.js
 * ë©”ëª¨ë¦¬ íš¨ìœ¨ì ì¸ íŒŒí‹°í´ í’€ ê´€ë¦¬ í´ë˜ìŠ¤
 * ê¸°ì¡´ ë¹„ì£¼ì–¼ê³¼ ê¸°ëŠ¥ì„ ì™„ì „íˆ ë³´ì¡´í•˜ë©´ì„œ ì„±ëŠ¥ë§Œ ê°œì„ 
 */

import Particle from './Particle.js';

class OptimizedParticlePool {
    constructor(size) {
        this.size = size;
        this.pool = new Array(size);
        this.activeParticles = [];
        this.activeCount = 0;
        
        // ì „ì²´ ì• ë‹ˆë©”ì´ì…˜ ìƒíƒœ ê´€ë¦¬
        this.allPositionsComplete = false;
        this.globalSizingStartTime = 0;
        this.lastPositionCheckTime = 0;
        
        // ì´ˆê¸°í™” ì‹œ í•œ ë²ˆë§Œ íŒŒí‹°í´ ê°ì²´ë“¤ì„ ìƒì„±
        for (let i = 0; i < size; i++) {
            this.pool[i] = new Particle(0, 0, { r: 120, g: 120, b: 120 }, 0, 0, 26);
        }
        
        console.log(`ParticlePool ì´ˆê¸°í™” ì™„ë£Œ: ${size}ê°œ íŒŒí‹°í´`);
    }
    
    /**
     * íŒŒí‹°í´ í’€ì—ì„œ í™œì„± íŒŒí‹°í´ë“¤ì„ ê°€ì ¸ì™€ì„œ ì´ˆê¸°í™”
     * @param {Array} imageData - ì´ë¯¸ì§€ í”½ì…€ ë°ì´í„°
     * @param {number} canvasWidth - ìº”ë²„ìŠ¤ ë„ˆë¹„
     * @param {number} canvasHeight - ìº”ë²„ìŠ¤ ë†’ì´
     * @param {number} stepPixel - í”½ì…€ ìŠ¤í…
     * @returns {Array} í™œì„±í™”ëœ íŒŒí‹°í´ ë°°ì—´
     */
    activateParticles(imageData, canvasWidth, canvasHeight, stepPixel) {
        this.activeParticles.length = 0; // ë°°ì—´ ì¬í• ë‹¹ ì—†ì´ ì´ˆê¸°í™”
        this.activeCount = Math.min(imageData.length, this.size);
        
        // ì „ì²´ ìƒíƒœ ì´ˆê¸°í™”
        this.allPositionsComplete = false;
        this.globalSizingStartTime = 0;
        this.lastPositionCheckTime = 0;
        
        for (let i = 0; i < this.activeCount; i++) {
            const particle = this.pool[i];
            const pixel = imageData[i];
            
            // ê¸°ì¡´ê³¼ ë™ì¼í•œ reset ë¡œì§
            particle.reset(pixel.x, pixel.y, pixel.color, canvasWidth, canvasHeight, stepPixel);
            this.activeParticles[i] = particle;
        }
        
        return this.activeParticles;
    }
    
    /**
     * ëª¨ë“  íŒŒí‹°í´ì„ ë¹„í™œì„±í™”
     */
    deactivateAll() {
        this.activeCount = 0;
        this.activeParticles.length = 0;
    }
    
    /**
     * í™œì„± íŒŒí‹°í´ ê°œìˆ˜ ë°˜í™˜
     */
    getActiveCount() {
        return this.activeCount;
    }
    
    /**
     * í™œì„± íŒŒí‹°í´ ë°°ì—´ ë°˜í™˜ (ì½ê¸° ì „ìš©)
     */
    getActiveParticles() {
        return this.activeParticles;
    }
    
    /**
     * ëª¨ë“  íŒŒí‹°í´ì˜ ìœ„ì¹˜ ì™„ë£Œ ìƒíƒœë¥¼ í™•ì¸í•˜ê³  ì‚¬ì´ì¦ˆ ë³€í™” ì‹œì‘
     * @param {number} currentTime - í˜„ì¬ ì‹œê°„
     */
    updateGlobalState(currentTime) {
        // 0.1ì´ˆë§ˆë‹¤ ì²´í¬ (ì„±ëŠ¥ ìµœì í™”)
        if (currentTime - this.lastPositionCheckTime < 100) {
            return;
        }
        this.lastPositionCheckTime = currentTime;
        
        // ì•„ì§ ì „ì²´ ìœ„ì¹˜ ì™„ë£Œê°€ ì•ˆëë‹¤ë©´ ì²´í¬
        if (!this.allPositionsComplete) {
            let allInPosition = true;
            for (let i = 0; i < this.activeCount; i++) {
                const particle = this.activeParticles[i];
                if (!particle.positionComplete) {
                    allInPosition = false;
                    break;
                }
            }
            
            if (allInPosition) {
                this.allPositionsComplete = true;
                this.globalSizingStartTime = currentTime;
                console.log('All particles in position, starting global sizing animation');
                
                // ëª¨ë“  íŒŒí‹°í´ì—ê²Œ ì‚¬ì´ì¦ˆ ë³€í™” ì‹œì‘ ì•Œë¦¼
                for (let i = 0; i < this.activeCount; i++) {
                    const particle = this.activeParticles[i];
                    particle.startGlobalSizing(currentTime);
                }
            }
        }
    }
}


    // ì—…ë¡œë“œ ë‹¨ê³„ í‘œì‹œ
    showUploadStep(step) {
        // ëª¨ë“  ë‹¨ê³„ ìˆ¨ê¸°ê¸°
        for (let i = 1; i <= 3; i++) {
            const stepElement = document.getElementById(`uploadStep${i}`);
            if (stepElement) {

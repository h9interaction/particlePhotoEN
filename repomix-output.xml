    // ì—‘ì…€ ë‹¤ìš´ë¡œë“œ ê¸°ëŠ¥
    downloadExcel() {
        try {
            // CSV í˜•ì‹ìœ¼ë¡œ ë°ì´í„° ìƒì„± (ì—‘ì…€ì—ì„œ ì—´ ìˆ˜ ìˆìŒ)
            const headers = ['í•œê¸€ì´ë¦„', 'ì˜ë¬¸ì´ë¦„', 'ì¡°ì§', 'ì§ë¬´', 'ì§ìœ„', 'ì´ë©”ì¼', 'ì´ë¯¸ì§€íŒŒì¼ëª…'];
            const csvContent = [
                headers.join(','), // í—¤ë” í–‰
                ...this.people.map(person => [
                    `"${person.koreanName || ''}"`,
                    `"${person.englishName || ''}"`,
                    `"${person.organization || ''}"`,
                    `"${person.role || ''}"`,
                    `"${person.position || ''}"`,
                    `"${person.email || ''}"`,
                    `"${person.imageFile || person.englishName + '.png'}"`
                ].join(','))
            ].join('\n');

            // BOM ì¶”ê°€ (UTF-8 ì¸ì½”ë”©ì„ ìœ„í•´)
            const bom = '\uFEFF';
            const blob = new Blob([bom + csvContent], { 
                type: 'text/csv;charset=utf-8;' 
            });

            // ë‹¤ìš´ë¡œë“œ ë§í¬ ìƒì„±
            const link = document.createElement('a');
            const url = URL.createObjectURL(blob);
            link.setAttribute('href', url);
            
            // íŒŒì¼ëª…ì— í˜„ì¬ ë‚ ì§œ í¬í•¨
            const now = new Date();
            const dateStr = now.toISOString().slice(0, 10); // YYYY-MM-DD í˜•ì‹
            const timeStr = now.toTimeString().slice(0, 8).replace(/:/g, '-'); // HH-MM-SS í˜•ì‹
            link.setAttribute('download', `particle_photo_data_${dateStr}_${timeStr}.csv`);
            
            // ë‹¤ìš´ë¡œë“œ ì‹¤í–‰
            link.style.visibility = 'hidden';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
            URL.revokeObjectURL(url);

            this.showMessage(`ë°ì´í„°ê°€ ì—‘ì…€ íŒŒì¼ë¡œ ë‹¤ìš´ë¡œë“œë˜ì—ˆìŠµë‹ˆë‹¤. (ì´ ${this.people.length}ê°œ í•­ëª©)`, 'success');
        } catch (error) {
            console.error('Excel download error:', error);
            this.showMessage('ì—‘ì…€ ë‹¤ìš´ë¡œë“œ ì¤‘ ì˜¤ë¥˜ê°€ ë°œìƒí–ˆìŠµë‹ˆë‹¤.', 'error');
        }
    }

    // ì—‘ì…€ ì—…ë¡œë“œ ê´€ë ¨ ì´ë²¤íŠ¸ ë°”ì¸ë”©

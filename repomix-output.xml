async function syncLegacyFilesFromFirestore() {
    if (!firebase || !firebase.db) {
        console.log('Firebaseê°€ ì„¤ì •ë˜ì§€ ì•Šì•„ ë™ê¸°í™”ë¥¼ ê±´ë„ˆëœë‹ˆë‹¤.');

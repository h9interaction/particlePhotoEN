    async deleteSelected() {
        // ì¤‘ë³µ ì‚­ì œ ë°©ì§€ ë° ì €ì¥ ì¤‘ì¼ ë•Œ ì‚­ì œ ë°©ì§€
        if (this.isDeleting || this.isSaving) {

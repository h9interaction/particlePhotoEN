async function processZipFile(zipBuffer) {
    return new Promise((resolve, reject) => {
        const zipData = {
            csv: null,
            images: {}
        };

        yauzl.fromBuffer(zipBuffer, { lazyEntries: true }, (err, zipfile) => {
            if (err) {
                reject(new Error('ZIP íŒŒì¼ì„ ì½ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤: ' + err.message));
                return;
            }

            let processedEntries = 0;
            let allEntries = [];

            // ë¨¼ì € ëª¨ë“  ì—”íŠ¸ë¦¬ë¥¼ ìˆ˜ì§‘
            zipfile.on('entry', (entry) => {
                console.log(`ZIP ì—”íŠ¸ë¦¬ ë°œê²¬: ${entry.fileName} (ë””ë ‰í† ë¦¬: ${/\/$/.test(entry.fileName)})`);
                allEntries.push(entry);
                zipfile.readEntry(); // ë‹¤ìŒ ì—”íŠ¸ë¦¬ ì½ê¸°
            });

            zipfile.on('end', () => {
                console.log(`ì´ ${allEntries.length}ê°œì˜ ì—”íŠ¸ë¦¬ ë°œê²¬`);
                
                if (allEntries.length === 0) {
                    resolve(zipData);
                    return;
                }

                // ëª¨ë“  ì—”íŠ¸ë¦¬ë¥¼ ìˆœì°¨ì ìœ¼ë¡œ ì²˜ë¦¬
                let currentIndex = 0;
                
                const processNextEntry = () => {
                    if (currentIndex >= allEntries.length) {
                        console.log('ëª¨ë“  ZIP ì—”íŠ¸ë¦¬ ì²˜ë¦¬ ì™„ë£Œ');
                        resolve(zipData);
                        return;
                    }

                    const entry = allEntries[currentIndex];
                    currentIndex++;
                    
                    if (/\/$/.test(entry.fileName)) {
                        // ë””ë ‰í† ë¦¬ëŠ” ê±´ë„ˆë›°ê¸°
                        console.log(`ë””ë ‰í† ë¦¬ ê±´ë„ˆëœ€: ${entry.fileName}`);
                        processNextEntry();
                        return;
                    }

                    console.log(`íŒŒì¼ ì²˜ë¦¬ ì‹œì‘: ${entry.fileName}`);
                    zipfile.openReadStream(entry, (err, readStream) => {
                        if (err) {
                            reject(new Error(`íŒŒì¼ì„ ì½ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤: ${entry.fileName}`));
                            return;
                        }

                        const chunks = [];
                        readStream.on('data', (chunk) => {
                            chunks.push(chunk);
                        });

                        readStream.on('end', () => {
                            const buffer = Buffer.concat(chunks);
                            const fileName = path.basename(entry.fileName);
                            console.log(`íŒŒì¼ ì²˜ë¦¬ ì™„ë£Œ: ${fileName} (ì „ì²´ ê²½ë¡œ: ${entry.fileName}, í¬ê¸°: ${buffer.length})`);

                            // CSV íŒŒì¼ ì²˜ë¦¬
                            if (fileName.toLowerCase().endsWith('.csv')) {
                                zipData.csv = buffer.toString('utf-8');
                                console.log(`CSV íŒŒì¼ ë¡œë“œ: ${fileName}`);
                            }
                            // ì´ë¯¸ì§€ íŒŒì¼ ì²˜ë¦¬ (ê²½ë¡œì— ê´€ê³„ì—†ì´ íŒŒì¼ëª…ë§Œ ì‚¬ìš©)
                            else if (/\.(png|jpg|jpeg)$/i.test(fileName)) {
                                zipData.images[fileName] = buffer;
                                console.log(`ì´ë¯¸ì§€ íŒŒì¼ ë°œê²¬: ${fileName} (${entry.fileName}) - ${buffer.length} bytes`);
                            } else {
                                console.log(`ì•Œ ìˆ˜ ì—†ëŠ” íŒŒì¼ í˜•ì‹: ${fileName}`);
                            }

                            // ë‹¤ìŒ ì—”íŠ¸ë¦¬ ì²˜ë¦¬
                            processNextEntry();
                        });

                        readStream.on('error', (err) => {
                            reject(new Error(`íŒŒì¼ ì½ê¸° ì˜¤ë¥˜: ${entry.fileName} - ${err.message}`));
                        });
                    });
                };

                // ì²« ë²ˆì§¸ ì—”íŠ¸ë¦¬ë¶€í„° ì²˜ë¦¬ ì‹œì‘
                processNextEntry();
            });

            zipfile.on('error', (err) => {
                reject(new Error('ZIP íŒŒì¼ ì²˜ë¦¬ ì˜¤ë¥˜: ' + err.message));
            });

            // ì²« ë²ˆì§¸ ì—”íŠ¸ë¦¬ ì½ê¸° ì‹œì‘
            zipfile.readEntry();
        });
    });
}


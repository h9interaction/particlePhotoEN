    // ZIP íŒŒì¼ ì²˜ë¦¬
    async handleZipFile(file) {
        try {
            // ZIP íŒŒì¼ í¬ê¸° í™•ì¸ (ìµœëŒ€ 50MB)
            if (file.size > 50 * 1024 * 1024) {
                this.showMessage('ZIP íŒŒì¼ í¬ê¸°ê°€ ë„ˆë¬´ í½ë‹ˆë‹¤. (ìµœëŒ€ 50MB)', 'error');
                return;
            }

            this.showMessage('ZIP íŒŒì¼ì„ ë¶„ì„í•˜ê³  ìˆìŠµë‹ˆë‹¤...', 'info');
            
            // 3ë‹¨ê³„ë¡œ ì§ì ‘ ì´ë™ (ì—…ë¡œë“œ ì§„í–‰)
            this.showUploadStep(3);
            
            // ì„¸ì…˜ ID ìƒì„±
            const sessionId = 'upload_' + Date.now() + '_' + Math.random().toString(36).substr(2, 9);
            
            // ì§„í–‰ë¥  í‘œì‹œ ìš”ì†Œ
            const progressFill = document.getElementById('progressFill');
            const progressText = document.getElementById('progressText');
            const progressTitle = document.getElementById('uploadProgressTitle');
            
            // SSE ì—°ê²° ì„¤ì •
            const eventSource = new EventSource(`/api/upload-progress/${sessionId}`);
            let uploadCompleted = false;
            
            eventSource.onmessage = (event) => {
                try {
                    const data = JSON.parse(event.data);
                    
                    if (data.type === 'connected') {
                        console.log('ì§„í–‰ ìƒí™© ìŠ¤íŠ¸ë¦¼ ì—°ê²°ë¨:', data.sessionId);
                        return;
                    }
                    
                    // ì§„í–‰ ìƒí™© ì—…ë°ì´íŠ¸
                    progressFill.style.width = `${data.progress}%`;
                    progressText.textContent = data.message;
                    
                    if (data.type === 'processing') {
                        progressTitle.textContent = 'ZIP íŒŒì¼ ì²˜ë¦¬ ì¤‘...';
                        if (data.currentUser) {
                            progressText.textContent = `${data.message} (${data.currentUser})`;
                        }
                    } else if (data.type === 'uploading') {
                        progressTitle.textContent = 'ì´ë¯¸ì§€ ì—…ë¡œë“œ ì¤‘...';
                    } else if (data.type === 'completed') {
                        progressTitle.textContent = 'ì—…ë¡œë“œ ì™„ë£Œ';
                        progressText.textContent = 'ëª¨ë“  ì‘ì—…ì´ ì™„ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.';
                        uploadCompleted = true;
                        eventSource.close();
                    } else if (data.type === 'error') {
                        progressTitle.textContent = 'ì˜¤ë¥˜ ë°œìƒ';
                        progressText.textContent = data.message;
                        uploadCompleted = true;
                        eventSource.close();
                    }
                    
                } catch (e) {
                    console.error('ì§„í–‰ ìƒí™© íŒŒì‹± ì˜¤ë¥˜:', e);
                }
            };
            
            eventSource.onerror = (error) => {
                console.error('SSE ì—°ê²° ì˜¤ë¥˜:', error);
                eventSource.close();
            };
            
            // FormData ìƒì„±
            const formData = new FormData();
            formData.append('zipFile', file);
            formData.append('conflictMode', 'skip');
            formData.append('sessionId', sessionId);
            
            // ì´ˆê¸° ìƒíƒœ ì„¤ì •
            progressTitle.textContent = 'ZIP íŒŒì¼ ì—…ë¡œë“œ ì¤‘...';
            progressText.textContent = 'íŒŒì¼ ì „ì†¡ ì¤‘...';
            progressFill.style.width = '5%';

            // API í˜¸ì¶œ
            const response = await fetch('/api/people/bulk-upload-zip', {
                method: 'POST',
                body: formData
            });

            const result = await response.json();

            // SSEê°€ ì™„ë£Œ ìƒíƒœë¥¼ ë°›ì§€ ëª»í•œ ê²½ìš° ëŒ€ë¹„
            if (!uploadCompleted) {
                eventSource.close();
            }

            if (response.ok) {
                // ì„±ê³µ ì²˜ë¦¬
                if (!uploadCompleted) {
                    progressFill.style.width = '100%';
                    progressText.textContent = 'ì™„ë£Œ';
                    progressTitle.textContent = 'ì—…ë¡œë“œ ì™„ë£Œ';
                }
                
                // ê²°ê³¼ í‘œì‹œ
                this.showUploadResults(result.results);
                
                // ë°ì´í„° ìƒˆë¡œê³ ì¹¨
                await this.loadPeople();
                this.renderItems();
                
                this.showMessage(result.message, 'success');
            } else {
                throw new Error(result.error || 'ZIP ì—…ë¡œë“œ ì‹¤íŒ¨');
            }

        } catch (error) {
            console.error('ZIP ì—…ë¡œë“œ ì˜¤ë¥˜:', error);
            this.showUploadError(error.message);
        }
    }
